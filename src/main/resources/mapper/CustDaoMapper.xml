<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lfl.dao.CustDao">


<select id="selectCustInfo" resultType="com.lfl.pojo.CustomInfo">
    select
        id,name,age
    from
        custom_info
    where
        id = #{id}
</select>

</mapper>